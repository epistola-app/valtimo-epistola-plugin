{
  "name": "@epistola.app/valtimo-plugin",
  "version": "1.0.0",
  "description": "Epistola document generation plugin for Valtimo",
  "license": "EUPL-1.2",
  "module": "dist/fesm2022/epistola.app-valtimo-plugin.mjs",
  "typings": "dist/index.d.ts",
