{
  "slug": "ontvangstbevestiging-bezwaar",
  "name": "Ontvangstbevestiging bezwaarschrift",
  "version": "1.0.0",
  "dataModel": {
    "type": "object",
    "properties": {
      "objector": {
        "type": "object",
        "properties": {
          "firstName": { "type": "string", "description": "Voornaam van de bezwaarmaker" },
          "lastName": { "type": "string", "description": "Achternaam van de bezwaarmaker" },
          "address": {
            "type": "object",
            "properties": {
              "street": { "type": "string", "description": "Straatnaam" },
              "houseNumber": { "type": "string", "description": "Huisnummer" },
              "postalCode": { "type": "string", "description": "Postcode" },
              "city": { "type": "string", "description": "Woonplaats" }
            },
            "required": ["street", "houseNumber", "postalCode", "city"]
          }
        },
        "required": ["firstName", "lastName", "address"]
      },
      "originalDecision": {
        "type": "object",
        "properties": {
          "reference": { "type": "string", "description": "Kenmerk van het oorspronkelijke besluit" },
          "date": { "type": "string", "format": "date-time", "description": "Datum van het oorspronkelijke besluit" },
          "subject": { "type": "string", "description": "Onderwerp van het oorspronkelijke besluit" }
        },
        "required": ["reference", "date", "subject"]
      },
      "objection": {
        "type": "object",
        "properties": {
          "grounds": { "type": "string", "description": "Gronden van het bezwaar" },
          "receivedDate": { "type": "string", "format": "date-time", "description": "Datum van ontvangst van het bezwaarschrift" }
        },
        "required": ["grounds", "receivedDate"]
      }
    },
    "required": ["objector", "originalDecision", "objection"]
  },
