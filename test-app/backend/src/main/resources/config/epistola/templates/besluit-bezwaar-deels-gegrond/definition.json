  "dataExamples": [
    {
      "id": "deels-gegrond-voorbeeld",
      "name": "Bezwaar deels gegrond verklaard",
      "data": {
        "objector": {
          "firstName": "Anneke",
          "lastName": "van den Berg",
          "address": {
            "street": "Laan van Meerdervoort",
            "houseNumber": "108",
